{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "data": {
    "type": "object",
    "required": [
      "id",
      "manufacturer",
      "model",
      "color",
      "stateSign",
      "productionYear",
      "accidentYears"
    ],
    "additionalProperties": true,
    "properties": {
      "id": {
        "type": "integer"
      },
      "manufacturer": {
        "type": "string",
        "minLength": 1
      },
      "model": {
        "type": "string",
        "minLength": 1
      },
      "color": {
        "type": "string",
        "minLength": 1
      },
      "stateSign": {
        "type": "string",
        "minLength": 1
      },
      "productionYear": {
        "type": "integer"
      },
      "accidentYears": {
        "type": "array",
        "items": {
          "type": "number"
        }
      }
    }
  }
}
